{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": false,
    "skipLibCheck": true,
    "types": ["react", "react-native"],
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "types/**/*.d.ts",
    "app/**/*.tsx",
    "app/**/*.ts"
  ]
}