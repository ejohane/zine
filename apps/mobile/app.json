{
  "expo": {
    "name": "zine",
    "slug": "zine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zine-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/zine-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erikjohansson.zine",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/zine-logo.png",
      "backgroundColor": "#000000"
    },
      "edgeToEdgeEnabled": true,
      "package": "com.erikjohansson.zine"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "zine",
    "extra": {
      "eas": {
        "projectId": "c14045ec-332f-40ed-92dc-f8814f0102c0"
      }
    },
  "plugins": [
    "expo-router",
    "expo-secure-store",
    [
      "expo-splash-screen",
      {
        "backgroundColor": "#000000",
        "image": "./assets/zine-logo.png",
        "imageWidth": 200
      }
    ]
  ]
  }
}
